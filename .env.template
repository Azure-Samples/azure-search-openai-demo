```dotenv
# Local development overrides for autonomous Codespaces.
# Populate each value with your Azure resource names/keys before running the app locally.

# Azure OpenAI
OPENAI_HOST=azure
AZURE_OPENAI_SERVICE=REPLACE_ME_OPENAI_SERVICE
AZURE_OPENAI_CHATGPT_MODEL=gpt-4.1-mini
AZURE_OPENAI_CHATGPT_DEPLOYMENT=REPLACE_ME_GPT_DEPLOYMENT
AZURE_OPENAI_EMB_DEPLOYMENT=REPLACE_ME_EMBEDDINGS_DEPLOYMENT
AZURE_OPENAI_EMB_MODEL_NAME=text-embedding-3-large
AZURE_OPENAI_EMB_DIMENSIONS=3072
AZURE_OPENAI_API_KEY_OVERRIDE=REPLACE_ME_AZURE_OPENAI_KEY
OPENAI_API_KEY=REPLACE_ME_STANDARD_OPENAI_KEY

# Storage + Search
AZURE_STORAGE_ACCOUNT=REPLACE_ME_STORAGE_ACCOUNT
AZURE_STORAGE_CONTAINER=content
AZURE_IMAGESTORAGE_CONTAINER=images
AZURE_SEARCH_SERVICE=REPLACE_ME_SEARCH_SERVICE
AZURE_SEARCH_INDEX=REPLACE_ME_SEARCH_INDEX
AZURE_SEARCH_AGENT=REPLACE_ME_SEARCH_AGENT
AZURE_SEARCH_FIELD_NAME_EMBEDDING=embedding
AZURE_STORAGE_RESOURCE_GROUP=REPLACE_ME_RESOURCE_GROUP
AZURE_SUBSCRIPTION_ID=REPLACE_ME_SUBSCRIPTION_ID

# Speech (optional)
AZURE_SPEECH_SERVICE_ID=REPLACE_ME_SPEECH_RESOURCE
AZURE_SPEECH_SERVICE_LOCATION=eastus

# Auth defaults (keep "false" unless you have Entra ID configured)
AZURE_USE_AUTHENTICATION=false
AZURE_ENFORCE_ACCESS_CONTROL=false
AZURE_ENABLE_UNAUTHENTICATED_ACCESS=true

# Misc feature gates
ENABLE_LANGUAGE_PICKER=true
USE_SPEECH_INPUT_BROWSER=false
USE_SPEECH_OUTPUT_AZURE=false
USE_SPEECH_OUTPUT_BROWSER=true
USE_CHAT_HISTORY_BROWSER=true
USE_CHAT_HISTORY_COSMOS=false
USE_USER_UPLOAD=false
USE_MULTIMODAL=false
USE_VECTORS=true

# Cosmos/chat history placeholders (optional)
AZURE_COSMOSDB_ACCOUNT=REPLACE_ME_COSMOS_ACCOUNT
AZURE_CHAT_HISTORY_DATABASE=REPLACE_ME_CHAT_DB
AZURE_CHAT_HISTORY_CONTAINER=REPLACE_ME_CHAT_CONTAINER
AZURE_CHAT_HISTORY_VERSION=cosmosdb-v2

# Leave this alone unless you want to keep existing env vars untouched.
LOADING_MODE_FOR_AZD_ENV_VARS=override

```
